{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"nodemon --quiet --watch src --ext ts,tsx --exec 'tsc --noEmit --pretty --listFiles && eslint src --ext .ts,.tsx --format stylish'\"",
    "build": "tsc -b && vite build",
    "lint": "eslint . --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@react-hook/merged-ref": "^1.3.2",
    "@reduxjs/toolkit": "^1.9.7",
    "ahooks": "^3.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-qr-code": "^2.0.17",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.30.0",
    "styled-components": "^6.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/node": "^24.0.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/styled-components": "^5.1.30",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "eslint-config-love": "^119.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.0",
    "vite-plugin-checker": "^0.9.1",
    "vite-plugin-svgr": "^4.3.0"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@swc/core",
      "esbuild"
    ]
  }
}